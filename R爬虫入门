rm(list = ls())
library('xml2')
library('rvest')
library('dplyr')
library('stringr')
i <- 1
for(i in 1:300){web <- read_html(paste('https://hrb.lianjia.com/ershoufang/pg/d', i, sep = ''), encoding = 'UTF-8')}
house_tag <- web %>% html_nodes('div.content') %>% html_text()
house_p <- web %>% html_nodes('div.price') %>% html_text()

